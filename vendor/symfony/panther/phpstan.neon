parameters:
	ignoreErrors:
		# https://github.com/symfony/symfony/pull/27849
		- '/Parameter #1 \$value of method Symfony\\Component\\DomCrawler\\Field\\ChoiceFormField::select\(\) expects string, array<int, string> given\./'
